   "cell_type": "code",
   "execution_count": 759,
   "metadata": {},
   "outputs": [],
   "source": [
    "user_consumption_per_month = user_consumption_per_month.groupby(['user_id', 'month']).agg({\n",
    "    'total_min_month': 'sum',\n",
    "    'number_of_calls': 'sum',\n",
    "    'total_messages_month': 'sum',\n",
    "    'mb_used_month': 'sum',\n",
    "    'gb_used_month': 'sum'\n",
    "}).reset_index()"
   ]
